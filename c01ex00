#include <unistd.h>
// #include <stdio.h>

void ft_ft(int *nbr);

void ft_ft(int *nbr)
{
	*nbr = 42;
}

void ft_ft(int *********nbr)
{
	*********nbr = 42;
}

void swap(int *a, int *b)
{
	int temp;
	temp = *a;
	*a = *b;
	*b = temp;
}

void ft_div_mod(int a, int b, int *div, int *mod)
{
	*div = a / b;
	*mod = a % b;
}



void main()
{
    int i = 10;
	int *ptr;
	ptr = &i;
	printf("ptr is %d\n", *ptr);
	ft_ft(ptr);
	printf("now ptr is %d\n", *ptr);
}
