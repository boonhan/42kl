#include <unistd.h>
#include <stdio.h>
#include <string.h>

#include <stdio.h>
#include <unistd.h>
#include <string.h>

int ft_strcmp(char *s1, char *s2)
{
    int i;
    i = 0;
    while (s1[i++] !='\0')
    {
        if (s1[i] != s2[i])
            return (s1[i] - s2[i]);
        if (s2[i] == '\0')
            return (s1[i]);
    }
    return (s2[i - 1]);
}

int ft_strncmp(char *s1, char *s2, unsigned int n)
{
    int i;
    i = 0;
    while (i++ < n)
    {
        if (s1[i] == '\0' || s2[i] == '\0' || s1[i] != s2[i])
            return (s1[i] - s2[i]);
    }
    return (0);
}

char *ft_strcat(char *dest, char *src)
{
    char *result;
    int i;
    
    result = dest;
    printf("initial result is %s\n", result);
    
    i = 0;
    while (dest[i++] != '\0');
    printf("i is now %d\n", i);
    while (*(src++) != '\0')
    {
        result[i-1] = *(src-1);
        printf("i is now %d\n", i);
        printf("*src is now %c, result[i] is %s\n", *(src-1), result);
        i++;
    }
    dest[i-1] = '\0';
    return (0);
}


int main() {
    printf("Hello world\n");
    char i[] = "testing9";
    char j[] = "testing9";
    //strcmp finds the first difference, returns s1 - s2.
    
    printf("i is %p, &i is %p\n", i, &i);
    printf("i[1] is %p, i[2] is %p\n", (i+1), (i+2));
    printf("strcmp is %d\n", strcmp(i, j));
    printf("ft_str is %d\n", ft_strcmp(i, j));
    
    return 0;
}
